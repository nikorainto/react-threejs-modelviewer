{
  "root": true,
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "worker": true,
    "jest": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "parser": "babel-eslint",
  "rules": {
    "no-console": "off",
    "react/prop-types": [
      "error",
      {
        "skipUndeclared": true
      }
    ]
  },
  "plugins": ["react"]
}
